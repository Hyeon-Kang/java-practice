//12주차 실습 02
public class ArrayPractice {

	public static void main(String argsp[]) {
		int b[][]= {{1,2,3},{4,5,6},{7,8,9}};
		int s = 0;
		
		
		for(int i =0; i<b.length; i++) {
			for(int j = 0; j<b[i].length; j++) {
				s+=b[i][j];	
			}
		}
		
		s=0;
		
		
		for(int[]b1 : b)
			for(int b2:b1) {
				s+=b2;
			}
	}
}
